{
  "expo": {
    "name": "GameLoop",
    "slug": "GameLoop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon01.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon01.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.smh125.GameLoop"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon01.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon01.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "82c5bd2c-7ba8-4c38-a9b3-900c292660ef"
      }
    }
  }
}
